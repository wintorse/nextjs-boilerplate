{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-s lint:* format",
    "lint:js": "eslint --fix",
    "lint:css": "stylelint \"**/*.scss\"",
    "format": "prettier --write --ignore-path .gitignore --ignore-path .eslintignore --log-level warn \"**/*.{js,jsx,ts,tsx,json,yml}\""
  },
  "dependencies": {
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.83.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "3.4.2",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5"
  },
  "volta": {
    "node": "20.13.1",
    "npm": "10.8.1"
  }
}
